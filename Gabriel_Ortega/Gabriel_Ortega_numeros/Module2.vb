    Sub misturar1()
        Dim pos(24), i, a, c As Integer
        Dim trueorfalse As Boolean

        i = 1
        pos(a) = 1
        Do While i <= 24
            Randomize()
            c = CInt(Int((24 * Rnd()) + 1))
            For a = 1 To i
                If pos(a) = c Then
                    trueorfalse = True
                    Exit For
                End If
            Next
            If trueorfalse = True Then
                trueorfalse = False
            Else
