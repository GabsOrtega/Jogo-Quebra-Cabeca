    Sub misturar()
        Dim pos(15), i, a, c As Integer
        Dim trueorfalse As Boolean

        i = 1
        pos(a) = 1
        Do While i <= 15
            Randomize()
            c = CInt(Int((15 * Rnd()) + 1))
            For a = 1 To i
                If pos(a) = c Then
                    trueorfalse = True
                    Exit For
                End If
            Next
            If trueorfalse = True Then
                trueorfalse = False
            Else
                pos(i) = c
                i = i + 1
            End If
        Loop
        Form2.Button1.Text = pos(1)
        Form2.Button2.Text = pos(2)
        Form2.Button3.Text = pos(3)
